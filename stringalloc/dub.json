{
    "name": "stringalloc",
    "sourcePaths": [
        "source"
    ],
    "importPaths": [
        "source"
    ],
    "dependencies": {
        "automem": "~>0.6.4",
        "emsi_containers": "~>0.8.0"
    },
    "buildTypes": {
        "unittest-opt": {
            "buildOptions": ["unittests", "optimize", "inline"]
        }
    },
    "configurations": [
        {
            "name": "library",
            "targetType": "library"
        },
        {
            "name": "unittest",
            "targetType": "executable",
            "targetName": "stringalloc-unittest",
            "mainSourceFile": "source/app.d",
            "preGenerateCommands-windows": ["echo void main(){} > source\\app.d"],
            "postBuildCommands-windows": ["del source\\app.d"]
        }
    ],
    "dflags": ["-preview=in", "-preview=dip1000"],
	"-ddoxFilterArgs": ["--min-protection=Private"]
}